					<div class="uc-brand">
						<div class="site-name-and-slogan">
							{% include '@molecules/nav-logo/nav-logo.twig' %}
						</div>
					</div>
					<address>
						<p>{{ footer.addressLine1 }}</p>
						<p>{{ footer.addressLine2 }}</p>
						<p><a href="mailto:{{footer.email}}">{{ footer.email }}</a></p>
						<p>Phone: <a href="tel:+15106422564">{{ footer.phone }}</a></p>
					</address>
				</div>
				<!-- copyright + destiny logo -->
				<div>
					<p>Copyright Â© UC Regents</p>
					<img src="https://extension.berkeley.edu/images/logo_destiny_one.png" alt="Powered by Destiny One" />
				</div>
			</div>
