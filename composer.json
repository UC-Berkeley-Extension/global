{
  "name": "drupal/recommended-project",
  "description": "Drupal 9 project build for UC Berkeley Global website.",
  "type": "project",
  "support": {
    "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
    "chat": "https://www.drupal.org/node/314178"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/templates",
        "type": "drupal-library",
        "version": "4.11.1",
        "dist": {
          "type": "zip",
          "url": "https://download.ckeditor.com/templates/releases/templates_4.14.0.zip",
          "reference": "master"
        }
      }
    }
  ],
  "require": {
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.6",
    "drupal/admin_toolbar": "^2.3",
    "drupal/components": "2.0-beta3",
    "drupal/core": "9.0.6 as 8.9.0",
    "drupal/core-composer-scaffold": "^9",
    "drupal/core-project-message": "^9",
    "drupal/paragraphs": "^1.12",
    "drupal/pathauto": "^1.8",
    "drupal/pdb": "^1.0@alpha",
    "drupal/twig_field_value": "^2.0",
    "drupal/twig_tweak": "^2.6",
    "drush/drush": "^10.3",
    "zaporylie/composer-drupal-optimizations": "^1.1"
  },
  "require-dev": {
    "drupal/devel": "^4.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
        "php": "7.3"
    },
    "sort-packages": true,
    "preferred-install": {
        "drupal/pdb": "source"
    }
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      },
      "file-mapping": {
        "[web-root]/sites/default/development.services.yml": false
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "patches": {
      "drupal/core": {
        "Views twig template suggestions": "https://www.drupal.org/files/issues/2020-05-16/2118743-183-twig-debug-info.patch"
      },
      "drupal/pdb": {
        "Drupal 9 readiness (https://www.drupal.org/project/pdb/issues/3133320)": "https://www.drupal.org/files/issues/2020-09-25/drupal9-ready-3133320-6.patch"
      }
    }
  }
}
